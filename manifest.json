
{
  "manifest_version": 3,
  "name": "YouTube Timestamp Bookmark",
  "version": "1.0",
  "description": "Bookmark and manage YouTube timestamps easily.",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
